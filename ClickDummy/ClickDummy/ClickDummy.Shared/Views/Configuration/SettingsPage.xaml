        <TextBox x:Name="textBox_Copy" Text="Configuration Screen" TextWrapping="Wrap" Margin="715,298,672,48" TextChanged="textBox_TextChanged" Background="#66ACACAC" Style="{StaticResource MaterialOutlinedTextBoxStyle}" FontSize="24"/>
        <TextBox x:Name="textBox_Copy3" Text="Configuration Screen" TextWrapping="Wrap" Margin="1326,298,70,48" TextChanged="textBox_TextChanged" Background="#66ACACAC" Style="{StaticResource MaterialOutlinedTextBoxStyle}" FontSize="24"/>
